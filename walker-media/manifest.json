{
    "manifest_version": 2,
    "name": "Walker Web Page",
    "description": "Quick launch Walker Media",
    "version": "1.0.0",
    "icons": {
        "128": "logo-128.png"
    },
    "browser_action": {
        "default_icon": "logo.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}